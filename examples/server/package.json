{
  "private": true,
  "name": "server-example",
  "description": "Server development example, `tsdk` export `fe-sdk-demo` package.",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env MONOREPO_ROOT=../../ node ./node_modules/tsdk/bin/tsdk.js --nest build all",
    "example": "cross-env IS_TEST=true node dist-projects/dist-express-todo/main && npm run test",
    "serve": "node dist-projects/dist-express-todo/main",
    "dev": "nest start express-todo --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "sync-sdk": "npx tsdk --sync",
    "sync": "node ./node_modules/tsdk/bin/tsdk.js --sync",
    "test": "cross-env NODE_ENV=test TS_CONFIG_PATHS=true ts-mocha -p tsconfig.json src/**/*.test.ts"
  },
  "dependencies": {
    "tsdk-server-adapters": "*",
    "axios": "^0.27.2",
    "better-sqlite3": "^8.0.1",
    "bufferutil": "^4.0.7",
    "change-case": "^4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.4",
    "typeorm": "^0.3.11",
    "typeorm-cursor-pagination": "^0.10.0",
    "utf-8-validate": "^6.0.0",
    "ws": "^8.10.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.14",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.0",
    "@types/ws": "^8.5.3",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "socket.io-client": "^4.5.4",
    "tsdk": "*"
  },
  "license": "MIT"
}