    "serve": "serve docs"
  },
  "dependencies": {
    "zod": "^3.24.2",
    "axios": "^1.8.1",
    "xior": "^0.7.2",
    "kysely": "^0.27.5",
    "swr": "^2.3.2",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/vue-query": "^5.66.9",
    "i18next": "^23.10.1",
    "intl-pluralrules": "^2.0.1"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "typedoc": "^0.27.9",
    "concurrently": "^9.1.2"
  },
  "peerDependencies": {
    "socket.io-client": "^4.8.1"
  },
  "peerDependenciesMeta": {
    "socket.io-client": {
      "optional": true
    }
  },
  "files": [
    "lib",
    "esm",
    "src"
