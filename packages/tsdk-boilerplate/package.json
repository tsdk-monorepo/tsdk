{
  "name": "tsdk-boilerplate",
  "version": "0.0.20-beta.3",
  "main": "lib/index.js",
  "scripts": {
    "start-publish": "npm run build && npm publish --registry=https://registry.npmjs.org",
    "build": "rm -rf lib && tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "checktype": "tsc --noEmit"
  },
  "dependencies": {
    "tsdk": "*",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@configs/ts-config": "*",
    "@configs/eslint-config": "*",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0"
  },
  "eslintConfig": {
    "extends": [
      "@configs/eslint-config/web",
      "@configs/eslint-config/native"
    ],
    "ignorePatterns": [
      "node_modules"
    ]
  },
  "files": [
    "lib/*",
    "README.md"
  ],
  "keywords": [
    "tsdk"
  ],
  "license": "MIT"
}
