{
  "name": "tsdk-server-adapters",
  "version": "0.0.20-beta.15",
  "main": "lib/index.js",
  "scripts": {
    "start-publish": "npm run build && npm publish --registry=https://registry.npmjs.org",
    "build": "rm -rf lib && tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "checktype": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test TS_CONFIG_PATHS=true ts-mocha -p tsconfig.json src/**/*.test.ts --parallel --timeout 30000"
  },
  "files": [
    "lib/*",
    "README.md"
  ],
  "keywords": [
    "tsdk",
    "http",
    "express",
    "ws",
    "socket.io"
  ],
  "dependencies": {
    "eventemitter3": "^4.0.7"
  },
  "devDependencies": {
    "@configs/ts-config": "*",
    "@configs/eslint-config": "*",
    "@configs/test-config": "*",
    "@types/express": "^4.17.17"
  },
  "peerDependencies": {
    "express": "^4.18.2",
    "zod": "^3",
    "socket.io": "^4.7.2",
    "ws": "^8.13.0",
    "hono": "^3.5.4"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "socket.io": {
      "optional": true
    },
    "ws": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@configs/eslint-config/node"
    ],
    "ignorePatterns": [
      "node_modules"
    ]
  },
  "license": "MIT"
}
