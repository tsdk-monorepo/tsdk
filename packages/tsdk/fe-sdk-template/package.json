{
  "name": "fe-sdk-template",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "require": "./lib/index.js",
  "scripts": {
    "tsc:build": "concurrently \"npm run tsc:build:cjs\"  \"npm run tsc:build:esm\"",
    "tsc:build:cjs": "tsc --project tsconfig.json",
    "tsc:build:esm": "tsc --project tsconfig.esm.json",
    "doc:build": "typedoc src/**.ts --plugin ./hover.mjs && node write-popover.mjs",
    "serve": "serve docs"
  },
  "dependencies": {
    "zod": "^3.24.2",
    "axios": "^1.8.1",
    "xior": "^0.6.3"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "typedoc": "^0.27.9",
    "concurrently": "^9.1.2"
  },
  "peerDependencies": {
    "socket.io-client": "^4.8.1"
  },
  "peerDependenciesMeta": {
    "socket.io-client": {
      "optional": true
    }
  },
  "files": [
    "lib",
    "esm",
    "src"
  ]
}
